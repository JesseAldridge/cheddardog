{
    "name": "cheddardog",
    "version": "2.0.47",
    "description": "fetch transactions from your financial institutions, like bofa and amex, then generate & email summary reports.",
    "main": "dist/src/index.js",
    "scripts": {
        "build": "tsc -m commonjs",
        "clean": "rm -rf ./dist",
        "clean-db": "rm -f ./txndb.json",
        "clean-report": "rm -f ./index.html",
        "clean-node-modules": "rm -rf node_modules",
        "clean-all": "npm run clean && npm run clean-db && npm run clean-report && npm run clean-node-modules",
        "start": "node dist/src/index.js",
        "refresh": "npm run start",
        "send": "node dist/src/sendreport.js"
    },
    "files": [
        "dist"
    ],
    "types": "dist/src/index.d.ts",
    "author": "Sam Khavari <skhavari@cs.stanford.edu>",
    "license": "UNLICENSED",
    "dependencies": {
        "@sendgrid/mail": "^6.3.1",
        "@types/csvtojson": "^1.1.5",
        "@types/node": "^10.12.18",
        "@types/puppeteer": "^1.11.1",
        "@types/shelljs": "^0.8.1",
        "csvtojson": "^2.0.8",
        "get-cursor-position": "^1.0.5",
        "puppeteer": "^1.11.0",
        "shelljs": "^0.8.3"
    }
}
